{
  "name": "primolotto-client",
  "version": "0.0.0",
  "description": "Primolotto client to play grids",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "tslint -p tsconfig.json --fix",
    "docs": "typedoc --out docs src",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
    "cover": "nyc mocha",
    "ci": "npm run lint && npm run build && npm run cover",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run ci",
    "preversion": "npm run ci",
    "clean:all": "npm run clean:tmp && rimraf node_modules",
    "clean:tmp": "rimraf dist docs coverage .nyc_output"
  },
  "author": "rem42 <me@remy.ovh> (https://remy.ovh)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rem42/primolotto-client.git"
  },
  "homepage": "https://github.com/rem42/primolotto-client#readme",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.2",
    "@types/cheerio": "^0.22.13",
    "@types/istanbul": "^0.4.30",
    "@types/mocha": "^7.0.0",
    "@types/node": "^13.13.2",
    "@types/tough-cookie": "^4.0.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "gh-pages": "^2.1.1",
    "mocha": "^7.0.1",
    "nyc": "^15.0.1",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.3.0",
    "tslint": "^6.1.1",
    "typedoc": "^0.16.0",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "axios-cookiejar-support": "^1.0.1",
    "cheerio": "^1.0.0-rc.3",
    "tough-cookie": "^4.0.0"
  },
  "bugs": {
    "url": "https://github.com/rem42/primolotto-client/issues"
  }
}
